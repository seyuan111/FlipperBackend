{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "functions": {
      "server.js": {
        "memory": 3008,
        "maxDuration": 10
      }
    }
  }